<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.niit.springboothomework">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Foodie">
        <activity android:name=".AdminAddActivity"></activity>
        <activity android:name=".AdminCheckActivity"></activity>
        <activity android:name=".AdminEditActivity"></activity>
        <activity android:name=".AdministratorActivity"></activity>
        <activity android:name=".StudentActivity"></activity>
        <activity android:name=".StudentsAskActivity"></activity>
        <activity android:name=".StudentsModifyActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>